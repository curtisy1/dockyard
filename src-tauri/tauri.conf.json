{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "dockyard",
    "version": "0.4.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "app": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "Dockyard",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "ropali.dockyard.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/ropali/dockyard/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc1RjkxM0Y1NTQ1RjY2RkMKUldUOFpsOVU5UlA1ZGVydXRQMERJUjk5dnA3eUt1WU5YUGx1WDFHV2RhWjR1MkV6TWhqRzBLTWQK"
    }
  }
}
